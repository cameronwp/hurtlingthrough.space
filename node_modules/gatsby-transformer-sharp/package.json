  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.0",
    "fs-extra": "^4.0.2",
    "image-size": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.0.5"
  }
}
