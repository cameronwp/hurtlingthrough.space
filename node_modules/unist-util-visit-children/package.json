{
  "name": "unist-util-visit-children",
  "version": "1.1.1",
  "description": "Unist utility to visit direct children of a parent",
  "license": "MIT",
  "keywords": [
    "unist",
    "util",
    "utility",
    "visit",
    "children"
  ],
  "repository": "https://github.com/syntax-tree/unist-util-visit-children",
  "bugs": "https://github.com/syntax-tree/unist-util-visit-children/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.0",
    "nyc": "^11.0.0",
    "remark-cli": "^3.0.0",
    "remark-preset-wooorm": "^3.0.0",
    "tape": "^4.6.2",
    "xo": "^0.18.2"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify index.js --no-builtins -s unistUtilVisitChildren > unist-util-visit-children.js",
    "build-mangle": "esmangle unist-util-visit-children.js > unist-util-visit-children.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "ignores": [
      "unist-util-visit-children.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
