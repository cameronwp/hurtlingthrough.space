  "bugs": {
    "url": "https://github.com/cameronwp/personal-web/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.162.0",
    "dotenv": "^4.0.0",
    "gatsby-remark-katex": "^1.0.7",
    "prompt": "^1.0.0",
    "s3": "^4.4.0"
  },
  "devDependencies": {
    "copy-paste": "^1.3.0",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "gatsby": "^1.9.128",
    "gatsby-link": "^1.6.28",
    "gatsby-plugin-feed": "^1.3.13",
    "gatsby-plugin-google-analytics": "^1.0.12",
    "gatsby-plugin-offline": "^1.0.10",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-sass": "^1.0.14",
    "gatsby-plugin-sharp": "^1.6.21",
    "gatsby-plugin-typography": "^1.7.10",
    "gatsby-remark-copy-linked-files": "^1.5.21",
    "gatsby-remark-prismjs": "^1.2.9",
    "gatsby-remark-responsive-iframe": "^1.4.14",
    "gatsby-remark-smartypants": "^1.4.8",
    "gatsby-source-filesystem": "^1.5.8",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^1.6.14",
    "lodash": "^4.17.19",
    "prettier": "^1.8.2",
    "react-responsive-grid": "^0.3.4",
    "react-twitter-widgets": "^1.7.1",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "homepage": "https://github.com/cameronwp/personal-web#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronwp/personal-web.git"
  },
  "scripts": {
    "dev": "./node_modules/gatsby-cli/lib/index.js develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "new": "./bin/new-post.js",
    "build": "./node_modules/gatsby-cli/lib/index.js build",
    "deploy": "./node_modules/gatsby-cli/lib/index.js build && ./bin/deploy.js",
    "serve": "./node_modules/gatsby-cli/lib/index.js serve",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "bin": {
    "personal-web": "deploy.js"
  }
}
